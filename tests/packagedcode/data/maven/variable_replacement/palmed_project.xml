<?xml version="1.0" encoding="UTF-8"?>
<project>
  <pomVersion>3</pomVersion>
  <artifactId>palmed</artifactId>
  <name>PalmEd</name>
  <groupId>palmed</groupId>
  <shortDescription>An ascii files editor for PDA</shortDescription>
  <id>palmed</id>
  <currentVersion>1.0.4</currentVersion>
  <inceptionYear>2004</inceptionYear>
  <organization>
    <name>PalmEd</name>
    <logo>http://sourceforge.net/sflogo.php?group_id=82605&amp;type=5</logo>
    <url>http://${pom.groupId}.sourceforge.net</url>
  </organization>
  <url>http://${pom.groupId}.sourceforge.net</url>
  <siteAddress>shell.sourceforge.net</siteAddress>
  <siteDirectory>/home/groups/${pom.groupId.substring(0,1)}/${pom.groupId.substring(0,2)}/${pom.groupId}/htdocs</siteDirectory>
  <package>palmed</package>
  <mailingLists>
    <mailingList>
    </mailingList>
  </mailingLists>
  <repository>
    <connection>scm:cvs:pserver:anonymous@cvs.sourceforge.net:/cvsroot/${pom.groupId}:${pom.artifactId}</connection>
    <developerConnection>scm:cvs:ssh:${maven.username}@cvs.sourceforge.net:/cvsroot/${pom.groupId}:${pom.artifactId}</developerConnection>
    <url>http://cvs.sourceforge.net/cgi-bin/viewcvs.cgi/${pom.groupId}</url>
  </repository>
  <developers>
    <developer>
      <name>Mat</name>
      <id>mat</id>
      <email>mat007@users.sourceforge.net</email>
    </developer>
  </developers>
  <dependencies>
    <dependency>
      <groupId>easymock</groupId>
      <artifactId>easymock</artifactId>
      <version>1.1</version>
      <type>jar</type>
      <url>http://www.easymock.org</url>
    </dependency>
    <dependency>
      <groupId>easymock</groupId>
      <artifactId>easymockclassextension</artifactId>
      <version>1.1</version>
      <type>jar</type>
      <url>http://www.easymock.org</url>
    </dependency>
    <dependency>
      <groupId>asm</groupId>
      <artifactId>asm</artifactId>
      <version>1.4.2</version>
      <type>jar</type>
      <url>http://cglib.sourceforge.net</url>
    </dependency>
    <dependency>
      <groupId>cglib</groupId>
      <artifactId>cglib</artifactId>
      <version>2.0.1</version>
      <type>jar</type>
      <url>http://asm.objectweb.org</url>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <type>jar</type>
      <url>http://www.junit.org</url>
    </dependency>
    <dependency>
      <groupId>j2me</groupId>
      <artifactId>midp</artifactId>
      <version>2.0</version>
      <jar>midpapi20.jar</jar>
      <url>http://java.sun.com/products/j2mewtoolkit</url>
    </dependency>
    <dependency>
      <groupId>j2me</groupId>
      <artifactId>cldc</artifactId>
      <version>1.1</version>
      <jar>cldcapi11.jar</jar>
      <url>http://java.sun.com/products/j2mewtoolkit</url>
    </dependency>
    <dependency>
      <groupId>j2me</groupId>
      <artifactId>jsr75</artifactId>
      <version>1.0</version>
      <jar>jsr75.jar</jar>
      <url>http://java.sun.com/products/j2mewtoolkit</url>
    </dependency>
    <dependency>
        <groupId>maven-plugins</groupId>
        <artifactId>maven-cobertura-plugin</artifactId>
        <version>1.1</version>
        <type>plugin</type>
        <url>http://maven-plugins.sourceforge.net/maven-cobertura-plugin/</url>
    </dependency>
    <dependency>
        <groupId>maven-plugins</groupId>
        <artifactId>maven-findbugs-plugin</artifactId>
        <version>0.9.2</version>
        <type>plugin</type>
        <url>http://maven-plugins.sourceforge.net/maven-findbugs-plugin/</url>
    </dependency>
    <dependency>
        <groupId>maven-plugins</groupId>
        <artifactId>maven-javancss-plugin</artifactId>
        <version>1.3</version>
        <type>plugin</type>
        <url>http://maven-plugins.sourceforge.net/maven-javancss-plugin</url>
    </dependency>
  </dependencies>
  <build>
    <sourceDirectory>src</sourceDirectory>
    <unitTestSourceDirectory>test</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
      </includes>
    </unitTest>
    <resources>
      <resource>
        <directory>${basedir}/res</directory>
        <includes>
          <include>*.png</include>
        </includes>
      </resource>
    </resources>
  </build>
  <!-- see http://maven.apache.org/reference/user-guide.html#Reports -->
  <reports>
    <report>maven-license-plugin</report>
    <report>maven-javadoc-plugin</report>
    <report>maven-checkstyle-plugin</report>
    <report>maven-junit-report-plugin</report>
    <report>maven-javancss-plugin</report>
    <report>maven-cobertura-plugin</report>
    <report>maven-simian-plugin</report>
    <report>maven-jdepend-plugin</report>
    <report>maven-findbugs-plugin</report>
    <report>maven-jxr-plugin</report>
    <report>maven-faq-plugin</report>
    <report>maven-linkcheck-plugin</report>
  </reports>
</project>