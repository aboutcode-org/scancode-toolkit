  "<core:if test=\"<%= serverInfos.getMaxMemory() = 0 % \"",
  "",
  "title=\"maximum amount of memory that the Java virtual machine will attempt to use\" Max Memory",
  "<format:formatNumber value=\"<%= serverInfos.getMaxMemory() % \" type=\"bytes\"/",
  "",
  "</core:if",
  "",
  "",
  "",
