  "",
  "",
  "",
  "",
  "",
  "CPU and Memory",
  "style=\"text-align: left;\" border=\"0\"",
  "<core:if test=\"<%= serverInfos.getCpuCount() = 0 % \"",
  "",
  "title=\"maximum number of processors available to the Java virtual machine\" Number of CPUs",
  "align=\"center\" <format:formatNumber value=\"<%= serverInfos.getCpuCount() % \" type=\"number\"/",
  "",
  "</core:if",
  "",
  "title=\"amount of free memory in the system\" Free Memory",
  "<format:formatNumber value=\"<%= serverInfos.getFreeMemory() % \" type=\"bytes\"/",
