#ifndef VNCTYPES_H
#define VNCTYPES_H

#if(defined __cplusplus)
extern "C"
{
#endif

#include <X11/Xmd.h>


typedef struct {
  int shareDesktop; /* bool */
  int viewOnly; /* bool */

  const char* encodingsString;

  int useBGR233; /* bool */
  int nColours;
  int useSharedColours; /* bool */
  int requestedDepth;

  int rawDelay;
  int copyRectDelay;

  int debug; /* bool */

  int compressLevel;
  int qualityLevel;
  int dotCursor; /* bool */

} AppData;


enum InitStatus {
  INIT_OK = 0,
  INIT_NAME_RESOLUTION_FAILURE = 1,
  INIT_PROTOCOL_FAILURE = 2,
  INIT_CONNECTION_FAILED = 3,
  INIT_AUTHENTICATION_FAILED = 4,
  INIT_NO_SERVER = 5,
  INIT_SERVER_BLOCKED = 6,
  INIT_ABORTED = 7
};


#if(defined __cplusplus)
}
#endif

#endif
