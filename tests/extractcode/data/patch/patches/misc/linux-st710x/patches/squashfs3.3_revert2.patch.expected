      "-\t\t\t\t\tbreak;", 
      "-\t\t\t\ti = (i + 1) % squashfs_cached_blks;", 
      "-\t\t\t}", 
      "-", 
      "-\t\t\tmsblk->next_cache = (i + 1) % squashfs_cached_blks;", 
      "-", 
